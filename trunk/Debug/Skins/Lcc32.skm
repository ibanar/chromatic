[CHECK004]
Name=Intermediate file
NameDat=7|199|126|15
Dat000=
Dat001=%L-z

[CHECK005]
Name=Use lcclibc.dll
NameDat=7|215|126|15
Dat000=
Dat001=%O-libcdll|%P-dynamic

[CHECK006]
Name=Optimize
NameDat=133|135|100|15
Dat000=
Dat001=%D-O

[CHECK007]
Name=P6 instructions
NameDat=133|151|100|15
Dat000=
