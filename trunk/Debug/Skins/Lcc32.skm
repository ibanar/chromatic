[SKINFOS]
Title=Lcc32

[EXTENSION]
Ext=c

[EXTRAS]
TrapStdErr=1
ObjLstPrefix=

[PASCALNAME]
WINEXE=
DOSEXE=
WINDLL=
WINLIB=
WINSYS=
DOSCOM=

[INCLUDE]
IncType=#include "%0"

[INCLUDELIB]
EarlyInclude=0
PreInc=
InInc=
PostInc=
IncType=

[CONSTANTS]
Const=#define %0 %1

[RESP]
RespSep={NL}
RespDirect=
Resp000=""%A.obj""
Resp001={OBJS}
Resp002=""%A.res""

[LIBSPEC]
ForeChar=
BackChar=
RetrieveChar=

[RES]
AddResLanguage=1
ExtendedRes=1
AddVInfo=1
StopOnError=1
UseRawComma=0
Prog000=[TOOLSDIR]\rc.exe
Args000=/r "%A.trc"

[COMPILER]
StopOnError=1
Prog000=[BINDIR]\lcc.exe
Args000=%9 -Fo"%A.obj" %B %C %D %E %F %J %K %L %I %O %M %H

[LINKER]
HeadLib=0
PrefixLib=
StopOnError=1
DllEntryPoint=
DOSCom000=del
ProgWINEXE000=[BINDIR]\lcclnk.exe
ArgsWINEXE000=-subsystem windows %G %N @"%A.ode" %P -o "%A.exe"
ProgDOSEXE000=[BINDIR]\lcclnk.exe
ArgsDOSEXE000=-subsystem console %G %N @"%A.ode" %P -o "%A.exe"
ProgWINDLL000=[BINDIR]\lcclnk.exe
ArgsWINDLL000=-subsystem windows %G -DLL %N @"%A.ode" "%A.def" %P -o "%A.dll"
ProgWINLIB000=[BINDIR]\lcclib.exe
ArgsWINLIB000=/OUT:"%A.lib" @"%A.ode"

[TEMPHEADER]
Opt000=CHECK000
Opt001=CHECK001
Opt002=CHECK002
Opt003=CHECK003
Opt004=CHECK004
Opt005=CHECK005
Opt006=CHECK006
Opt007=CHECK007
Opt008=CHECK008
Opt009=CHECK009
Opt010=COMBO010
Opt011=COMBO011
Opt012=TEXTB012
Opt013=TEXTB013

